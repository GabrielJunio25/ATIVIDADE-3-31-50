#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
  // Inicializa o gerador de números aleatórios com a semente atual
  srand(time(NULL));

  // Gera um número aleatório entre 1 e 100 (inclusive)
  int random_number = rand() % 100 + 1;

  printf("Número aleatório: %d\n", random_number);

  return 0;
}
